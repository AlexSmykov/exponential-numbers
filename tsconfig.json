{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "src",
    "baseUrl": "./",
    "outDir": "dist",
    "declaration": true,
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "include": ["src"]
}
